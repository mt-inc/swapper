{
  "name": "swapper",
  "version": "1.0.0",
  "description": "Bot for swapping nealy mined tokens on pancakeswap",
  "main": "index.js",
  "author": "Maksym Shamko <max.shamko25@gmail.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "ethers": "^5.5.3",
    "pm2": "^5.1.2"
  },
  "scripts": {
    "push": "git push origin master",
    "pull": "git pull origin master",
    "commit": "git add . && git commit -m 'working'",
    "run": "ts-node --files --project tsconfig.json ./index.ts",
    "pm:saveData": "pm2 start ecosystem.Swapper.config.js",
    "pm:saveData:restart": "pm2 restart ecosystem.Swapper.config.js",
    "pm:saveData:stop": "pm2 stop ecosystem.Swapper.config.js",
    "pm:saveData:delete": "pm2 delete ecosystem.Swapper.config.js"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
